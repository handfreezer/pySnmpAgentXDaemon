{
    "log": {
        "level": "info"
    },
    "oid_base": "1.3.6.1.4.1.",
    "agents": {
        "logical_name": {
            "oid_ext": "1.2.3",
            "plugin": "process",
            "conf": {
                "list_pids": false,
                "logical_process": {
                    "oid_ext": "10",
                    "regex": "^.*/httpd$"
                }
            }
        },
        "logical_name2": {
            "oid_ext": "3.2.1",
            "plugin": "tailStat",
            "conf": {
                "filePath": "/tmp/file2tailForStat",
                "regex": "^.:([[digit]]*):.*:([[digit]]*):.*$"
            }
        }
    }
}
